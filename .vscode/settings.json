{
  "conventionalCommits.scopes": [
    "array",
    "strings",
    "hash-table",
    "queue",
    "stack",
    "recursion",
    "linked-list",
    "trees",
    "sorting-searching",
    "dynamic-programming",
    "design",
    "math",
    "others",
    "sql",
    "trie",
    "binary-search",
    "binary-search-tree",
    "n-ary-tree",
    "graph",
    "heap",
    "system-design",
    "machine-learning",
    "decision-tree",
    "bfs+dfs",
    "backtracking",
    "greedy",
    "sliding window",
    "two-pointers",
    "intervals",
    "counting",
    "union-find",
    "prefix-sum",
    "bit-manipulation",
    "weekly-contest",
    "matrix",
    "javascript",
    "pandas",
    "frontend"
  ],
  "search.exclude": {
    "package-lock.json": true,
    "yarn.lock": true
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.trimTrailingWhitespace": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.format": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit",
    "source.formatDocument": "explicit",
    "source.addMissingImports": "explicit"
  },
  "java.classPath": ["${workspaceFolder}/_utils/java"]
}
